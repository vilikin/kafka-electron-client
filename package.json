{
  "name": "kafka-ui",
  "version": "0.1.7",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "babel-plugin-prismjs": "^2.0.1",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.2.0",
    "kafkajs": "1.13.0-beta.28",
    "lodash": "^4.17.19",
    "overmind": "^23.1.0",
    "overmind-react": "^24.1.0",
    "prismjs": "^1.20.0",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-scripts": "3.4.1",
    "tcp-port-used": "^1.0.1",
    "typescript": "~3.7.2",
    "uuid": "^8.0.0"
  },
  "main": "./public/electron.js",
  "homepage": "./",
  "scripts": {
    "build:tailwind": "postcss src/tailwind.css -o src/tailwind.generated.css",
    "watch:tailwind": "postcss -w src/tailwind.css -o src/tailwind.generated.css",
    "start": "run-p watch:tailwind start:react",
    "start:react": "react-app-rewired start",
    "prebuild": "npm run build:tailwind",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"overmind-devtools\" \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "preelectron-pack-mac": "yarn build",
    "electron-pack": "electron-builder build -mw",
    "electron-pack-mac": "electron-builder build -m"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@types/classnames": "^2.2.10",
    "@types/lodash": "^4.14.152",
    "@types/prismjs": "^1.16.1",
    "@types/tcp-port-used": "^1.0.0",
    "@types/uuid": "^8.0.0",
    "autoprefixer": "^9.8.0",
    "concurrently": "^5.2.0",
    "customize-cra": "^1.0.0",
    "electron": "^8.5.2",
    "electron-builder": "^22.6.0",
    "eslint-plugin-prettier": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "overmind-devtools": "^24.1.0",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.6",
    "tailwindcss": "^1.4.6",
    "wait-on": "^5.0.0"
  },
  "author": {
    "name": "Vili Kinnunen",
    "url": "https://vilik.in/"
  },
  "build": {
    "appId": "in.vilik.kafkaui",
    "productName": "Kafka UI",
    "copyright": "Copyright Â© 2020 ${author}",
    "mac": {
      "category": "public.app-category.utilities",
      "publish": [
        "github"
      ]
    },
    "win": {
      "publish": [
        "github"
      ]
    },
    "linux": {
      "publish": [
        "github"
      ]
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "./backend/build/libs",
        "to": ".",
        "filter": [
          "*.jar"
        ]
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
